  "license": "MIT",
  "repository": "frozzare/gulp-phpcpd",
  "author": {
    "name": "Fredrik Forsmo",
    "email": "fredrik.forsmo@gmail.com",
    "url": "forsmo.me"
  },
  "engines": {
